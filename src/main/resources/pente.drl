package expert.detection
 
import expert.model.IntervalleMesure
import java.util.ArrayList;
import expert.model.Mesure;
import java.util.HashSet;
 
global java.util.HashSet penteList; 
 
rule "detection pente "

    when
        //conditions
        IntervalleMesure( $table : tabMesure)
        m : Mesure( alpha > 1.58) from $table
    then
        penteList.add(m);
        
end
